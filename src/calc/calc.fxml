<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPanel" prefHeight="328.0" prefWidth="298.0"
      xmlns="http://javafx.com/javafx/8" fx:controller="calc.Controller">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <TextField fx:id="aField" prefHeight="25.0" prefWidth="285.0">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
        <Pane prefHeight="200.0" prefWidth="200.0">
            <children>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="93.0" minWidth="10.0" prefWidth="50.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="152.0" minWidth="10.0" prefWidth="50.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="198.0" minWidth="10.0" prefWidth="50.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="232.0" minWidth="10.0" prefWidth="50.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Button fx:id="button1" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="1"
                                GridPane.columnIndex="0" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button2" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="2"
                                GridPane.columnIndex="1" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button3" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="3"
                                GridPane.columnIndex="2" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button4" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="4"
                                GridPane.rowIndex="1" GridPane.columnIndex="0" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button5" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="5"
                                GridPane.rowIndex="1" GridPane.columnIndex="1" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button6" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="6"
                                GridPane.rowIndex="1" GridPane.columnIndex="2" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button7" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="7"
                                GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button8" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="8"
                                GridPane.rowIndex="2" GridPane.columnIndex="1" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button9" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="9"
                                GridPane.rowIndex="2" GridPane.columnIndex="2" onAction="#digit"/>
                    </children>
                    <children>
                        <Button fx:id="button0" mnemonicParsing="false" prefHeight="25.0" prefWidth="48.0" text="0"
                                GridPane.rowIndex="3" GridPane.columnIndex="1" onAction="#digit"/>
                    </children>
                </GridPane>
            </children>
        </Pane>
        <Pane prefHeight="179.0" prefWidth="298.0">
            <children>
                <TextField fx:id="resultField" layoutX="27.0" layoutY="14.0" prefHeight="25.0" prefWidth="242.0"/>
            </children>
        </Pane>
    </children>
</VBox>

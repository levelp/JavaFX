<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="139.0" prefWidth="298.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="sample.Controller">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <TextField fx:id="aField">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
                <TextField fx:id="bField">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
        <Pane prefHeight="200.0" prefWidth="200.0">
            <children>
                <Button fx:id="calcButton" layoutX="123.0" layoutY="14.0" mnemonicParsing="false" onAction="#calcSum"
                        text="Сумма"/>
            </children>
        </Pane>
        <Pane prefHeight="200.0" prefWidth="200.0">
            <children>
                <TextField fx:id="resultField" layoutX="27.0" layoutY="14.0" prefHeight="25.0" prefWidth="242.0"/>
            </children>
        </Pane>
    </children>
</VBox>
